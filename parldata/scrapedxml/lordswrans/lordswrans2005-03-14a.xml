<ques id="uk.org.publicwhip/wrans/2005-03-14a.110.4" speakerid="uk.org.publicwhip/lord/100321" speakername="Lord Hylton" colon="" colnum="110"  url="http://www.publications.parliament.uk/pa/ld199900/ldhansrd/pdvn/lds05/text/50314w03.htm#50314w03_wqn1">
