			<tr pid="a.74.0/8"> <td>Driving etc. after consuming alcohol or taking drugs 3</td> <td>1996</td> <td>121</td> <td>110</td> <td>15,633</td> <td>13,958</td> <td>15,754</td> <td>14,068</td> </tr>
			<tr pid="a.74.0/9"> <td></td> <td>1997</td> <td>114</td> <td>103</td> <td>16,587</td> <td>15,102</td> <td>16,701</td> <td>15,205</td> </tr>
			<tr pid="a.74.0/10"> <td></td> <td>1998</td> <td>99</td> <td>83</td> <td>14,244</td> <td>12,854</td> <td>14,343</td> <td>12,937</td> </tr>
			<tr pid="a.74.0/11"> <td></td> <td>1999</td> <td>99</td> <td>92</td> <td>12,659</td> <td>11,433</td> <td>12,758</td> <td>11,525</td> </tr>
