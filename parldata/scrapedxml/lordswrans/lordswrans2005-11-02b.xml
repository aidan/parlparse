			<tr pid="b.27.5/29"> <td>2003&ndash;04</td> <td>7,024,000</td> </tr>
			<tr pid="b.27.5/30"> <td>2004&ndash;05</td> <td>6,779,490</td> </tr>
			<tr pid="b.27.5/31"> <td>2005&ndash;06</td> <td>7,137,000</td> </tr>
