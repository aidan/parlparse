<reply id="uk.org.publicwhip/wrans/2005-11-29b.28.2" speakerid="uk.org.publicwhip/lord/100839" speakername="Lord Adonis" colon=":" colnum="28"  url="http://www.publications.parliament.uk/pa/ld199900/ldhansrd/pdvn/lds05/text/51129w04.htm#51129w04_spnew5">
