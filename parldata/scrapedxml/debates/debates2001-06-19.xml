<speech id="uk.org.publicwhip/debate/2001-06-19.23.3" nospeaker="true" colnum="23"  time="14:30:00" url="http://www.publications.parliament.uk/pa/cm200102/cmhansrd/vo010619/debtext/10619-01.htm#10619-01_para200">
