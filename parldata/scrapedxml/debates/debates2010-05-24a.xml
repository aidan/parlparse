<minor-heading id="uk.org.publicwhip/debate/2010-05-24a.25.0" nospeaker="true" colnum="25"  time="14:30:00" url="http://www.publications.parliament.uk/pa/cm201011/cmhansrd/cm100524/debtext/100524-0001.htm#1005241000001">
	Members Sworn
</minor-heading>

<speech id="uk.org.publicwhip/debate/2010-05-24a.25.1" speakerid="uk.org.publicwhip/member/40131" speakername="Mr Speaker" colnum="25"  time="14:30:00" url="http://www.publications.parliament.uk/pa/cm201011/cmhansrd/cm100524/debtext/100524-0001.htm#1005241000104">
