    <business-item id="uk.org.publicwhip/future/d/20100228T125224/3/2010-03-16.1">
      <private-members-bill id="uk.org.publicwhip/future/d/20100228T125224/3/2010-03-16.1.1" speakerid="uk.org.publicwhip/member/1407" speakername="Gregory Barker">
        <item-heading id="uk.org.publicwhip/future/d/20100228T125224/3/2010-03-16.1.1.1">
          ILLEGALLY LOGGED TIMBER (PROHIBITION OF RETAIL, WHOLESALE AND DISTRIBUTION) BILL
        </item-heading>
        <motion-member id="uk.org.publicwhip/future/d/20100228T125224/3/2010-03-16.1.1.2">
          Gregory Barker
        </motion-member>
        <motion-text id="uk.org.publicwhip/future/d/20100228T125224/3/2010-03-16.1.1.3">
          That leave be given to bring in a Bill to prohibit the retail, wholesale and distribution in the United Kingdom of timber and wood products that were obtained or produced illegally in their country of origin; and for connected purposes.
        </motion-text>
      </private-members-bill>
    </business-item>
  </business-day>
  <business-day date="2010-03-17" id="uk.org.publicwhip/future/d/20100228T125224/4/2010-03-17">
