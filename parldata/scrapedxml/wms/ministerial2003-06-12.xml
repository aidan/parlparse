<major-heading id="uk.org.publicwhip/wms/2003-06-12.57WS.0" nospeaker="true" colnum="57WS"  url="http://www.publications.parliament.uk/pa/cm200203/cmhansrd/vo030612/wmstext/30612m01.htm#30612m01.html_dpthd1">
	DEFENCE
