			<tr pid="c.113WS.4/7"> <td></td> <td><i>Voted</i></td> <td><i>Non-voted</i></td> <td><i>Voted</i></td> <td><i>Non-voted</i></td> <td><i>Total</i></td> </tr>
			<tr pid="c.113WS.4/8"> <td>Resource</td> <td>113,222</td> <td>360,641</td> <td>6,043,726</td> <td>786,274</td> <td>6,830,000</td> </tr>
			<tr pid="c.113WS.4/9"> <td><i>of which:</i></td> <td></td> <td></td> <td></td> <td></td> <td></td> </tr>
